{
  "name": "userdocs_clients",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "docs": "doctoc --title '**Table of content**' README.md",
    "clean": "lerna run clean",
    "build": "lerna run build",
    "publish": "lerna publish",
    "compile": "lerna run compile fa-edit",
    "dist": "lerna run dist",
    "test": "lerna run test",
    "dev": "lerna run compile && lerna-watch @userdocs/desktop",
    "start_linux": "lerna run start_linux",
    "start_windows": "lerna run start_windows",
    "lint": "eslint --ext js,ts ."
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "lerna-watch": "^1.0.0",
    "tsc-watch": "^4.4.0",
    "typescript": "~4.2.3"
  }
}
