{
  "name": "@userdocs/cicd",
  "version": "1.0.25",
  "description": "CICD Runner for the UserDocs Screenshot collection app",
  "main": "dist/index",
  "repository": "https://github.com/johns10/userdocs_clients",
  "author": "John Davenport",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "build": "yarn run clean && yarn run compile",
    "dist": "yarn run clean && yarn run compile && pkg -t win-x64,linux,alpine,macos --out-path dist dist/index.js --public",
    "watch": "tsc-watch --noClear -p ./tsconfig.buildjson",
    "prepublishOnly": "yarn run build",
    "test": "jest"
  },
  "dependencies": {
    "@userdocs/extension": "^1.0.3",
    "@userdocs/runner": "^1.0.25",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "global": "^4.4.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "pkg": "^5.3.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "dotenv": "^8.2.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.1",
    "ts-node": "^9.1.1"
  },
  "gitHead": "99e0edfd14438859df62348157aec1eeaac2344e"
}
