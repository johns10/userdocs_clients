    "@types/puppeteer": "^5.4.3",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "dotenv": "^8.2.0",
    "jest-puppeteer": "^5.0.3",
    "ts-node": "^9.1.1",
    "webpack": "4.41.5",
