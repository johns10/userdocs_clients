{
  "name": "@userdocs/desktop",
  "version": "0.0.53",
  "description": "Authors steps and processes for collection of screenshots.",
  "repository": "https://github.com/johns10/userdocs_clients",
  "main": "build/index.js",
  "author": "John Davenport",
  "license": "MIT",
  "homepage": "https://user-docs.com",
  "build": {
    "appId": "johns10@gmail.com",
    "productName": "UserDocs",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "executableName": "user_docs",
      "target": "AppImage",
      "category": "Office"
    },
    "files": [
      "build/**/*"
    ],
    "asarUnpack": [
      "node_modules/@userdocs/extension/extension"
    ]
  },
  "scripts": {
    "copyHtml": "cpx ./src/gui ./build",
    "build": "postcss src/gui/style.css -o build/gui/style.css && yarn run clean && yarn run compile && npm run copyHtml",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "dist": "yarn run build && electron-builder",
    "pack": "electron-builder --dir",
    "prepublishOnly": "yarn run build",
    "publish": "yarn run build",
    "dev": "postcss src/gui/style.css -o build/gui/style.css && npm run copyHtml && tsc-watch --noClear -p ./tsconfig.build.json --onSuccess \"electron build/index.js\"",
    "start_linux": "./dist/linux-unpacked/user_docs",
    "start_windows": "./dist/win-unpacked/UserDocs.exe"
  },
  "dependencies": {
    "@userdocs/runner": "^1.0.26",
    "@userdocs/server": "^0.0.20",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "keytar": "^7.7.0",
    "ssl-root-cas": "^1.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "cpx": "^1.5.0",
    "electron": "12.0.9",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  },
  "gitHead": "012c88af0fd9253965193cbe3464f175f81a60ef"
}
