{
  "name": "@userdocs/desktop",
  "version": "0.0.30",
  "description": "Authors steps and processes for collection of screenshots",
  "repository": "https://github.com/johns10/userdocs_clients",
  "main": "build/index.js",
  "author": "John Davenport",
  "license": "MIT",
  "homepage": "https://user-docs.com",
  "build": {
    "appId": "johns10@gmail.com",
    "productName": "UserDocs Desktop",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "executableName": "user_docs",
      "target": "AppImage",
      "category": "Office"
    },
    "files": [
      "build/**/*"
    ]
  },
  "scripts": {
    "prepublishOnly": "yarn run build",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "build": "yarn run clean && yarn run compile && electron-builder",
    "publish": "yarn run build && electron-builder --mac --windows --linux --publish always",
    "start_desktop": "electron build/index.js",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@userdocs/runner": "^1.0.9",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9"
  },
  "devDependencies": {
    "electron": "12.0.9",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0"
  },
  "gitHead": "86f67f98f69cd74789133bdd56e23c8825065283"
}
