{
  "name": "@userdocs/server",
  "version": "0.0.21",
  "description": "Automation server",
  "main": "dist/index",
  "types": "dist/index",
  "repository": "https://github.com/johns10/userdocs_clients",
  "author": "John Davnport",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build",
    "watch": "tsc-watch --noClear -p ./tsconfig.build.json",
    "start": "yarn run build && tsc-watch --noClear -p ./tsconfig.build.json --onSuccess \"node dist/index.js\""
  },
  "dependencies": {
    "@types/node": "^16.4.0",
    "@userdocs/extension": "^1.0.5",
    "@userdocs/runner": "^1.0.27",
    "apollo-server": "^3.0.2",
    "graphql": "^15.5.1",
    "graphql-request": "^3.5.0"
  },
  "gitHead": "793ad8a78ba5c917268e12329c0bd79e163a745e"
}
